#!/bin/bash
#
docker build --tag tensorflow-2.13-gpu:latest -f tensorflow.dockerfile .
/opt/conda/envs/jupyterenv/bin/dockernel install tensorflow-2.13-gpu:latest \
    --name TensorFlow-2.13-GPU \
    -v labnotebook-homedirs:/home:rw \
    -v labnotebook-etc:/etc:rw \
    --gpus all \
    --network host \
    --user -1 \
    --group-add -1

